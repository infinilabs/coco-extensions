{
  "name": "Empty Trash",
  "description": "Empties the trash on macOS.",
  "icon": "icon.png",
  "type": "command",
  "platforms": [
    "macos"
  ],
  "screenshots": [
    {
      "title": "",
      "url": "screenshot.png"
    }
  ],
  "action": {
    "exec": "zsh",
    "args": [
      "-c",
      "osascript -e 'try' -e 'tell application \"Finder\" to empty' -e 'end try'"
    ]
  },
  "category": "Utilities",
  "tags": [
    "Productivity"
  ]
}
