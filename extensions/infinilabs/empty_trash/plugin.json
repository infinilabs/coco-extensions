{
  "name": "Empty Trash",
  "description": "Empties the trash on macOS.",
  "icon": "icon.png",
  "type": "command",
  "version": {
    "number": "0.2.0"
  },
  "minimum_coco_version": "0.9.0-SNAPSHOT-2560",
  "platforms": [
    "macos"
  ],
  "screenshots": [
    {
      "title": "",
      "url": "screenshot.png"
    }
  ],
  "action": {
    "exec": "zsh",
    "args": [
      "-c",
      "osascript -e 'tell application \"Finder\" to if (count items in trash) > 0 then empty trash'"
    ]
  },
  "category": "Utilities",
  "tags": [
    "Productivity"
  ]
}
